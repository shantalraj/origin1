
public class Arr {
	public static void main(String []args) {
		int [] numbers;
		numbers = new int[5];
		numbers[0]=100;
		numbers[1]=250;
		numbers[2]=100;
		numbers[3]=250;
		numbers[4]=200;//numbers[3]=450;
		int sum = numbers[0]+numbers[1]+numbers[2]+numbers[3]+numbers[4];
		//System.out.println("0th element = "+numbers[0]);
		System.out.println(sum);
	}
}
