package com.pojo;

import java.lang.reflect.Array;
//import java.util.ArrayList;

public class Student {

	String name;
	int[] marks = new int[5];
	int rollno;
	public Student()
	{
		name = "NoOne";
		marks[] = {0,0};
		rollno = 101;
	}
	public Student(String name, int [] marks, int rollno)
	{
		this.name = name;
		this.marks = marks;
		this.rollno = rollno;
	}
	
	public void display() {
		System.out.println(name+"\t"+marks+"\t"+rollno);
	}
}
