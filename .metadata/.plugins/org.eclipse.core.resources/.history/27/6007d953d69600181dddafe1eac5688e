
public class Arr {
	public static void main(String []args) {
		int [] numbers;
		
		numbers = new int[5];
		numbers[0]=args[0];
		numbers[1]=args[1];
		numbers[2]=args[2];
		numbers[3]=args[3];
		numbers[4]=args[4];
		
		int sum = numbers[0]+numbers[1]+numbers[2]+numbers[3]+numbers[4];
		
		System.out.println(sum);
	}
}
