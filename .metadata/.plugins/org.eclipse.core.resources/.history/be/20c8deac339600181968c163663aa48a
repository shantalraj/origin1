
public class Person {
	private int age;
	private String name;
	
	// access_sp return_type fun_name(){  }
	public void display()
	{
		System.out.println("Name:- "+name+"\t"+"Age:- "+age);	
	}
	public Person()
	{
		age = 18;
		name = "Chad";
	}
	public void setValues()
	{
		age = 20;
		name = "Shantal";
	}
	public void setValues(int a, String n)
	{
		age=a;
		name=n;
	}
	public void incrementAge(int a)
	{
		age=a+1;
	}
}
